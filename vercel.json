{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install --legacy-peer-deps",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_FORM_ENDPOINT": "@form_endpoint",
    "VITE_GA_MEASUREMENT_ID": "@ga_measurement_id"
  }
}